    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "del-cli": "^3.0.1"
  }
